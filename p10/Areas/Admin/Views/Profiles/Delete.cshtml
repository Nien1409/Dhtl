@model Model.EF.Profiles

@{
    ViewBag.Title = "Delete";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Profiles</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.name)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.name)
        </dd>
        <input type="hidden" name="ProfilesID" value="@Model.ProfilesID" />
    </dl>

    @using (Html.BeginForm("Delete", "Profiles", FormMethod.Post, new { @enctype = "multipart/form-data" }))
    {

        @Html.AntiForgeryToken()
        <input type="hidden" name="ProfilesID" value="@Model.ProfilesID" />
        <div class="form-actions">
            <a href="/Admin/Profiles" class="btn btn-dark">Quay Lại</a>
            <input type="submit" value="Delete" class="btn btn-default" /> |
        </div>
    }
</div>
